{
  "name": "ds-project",
  "version": "1.0.0",
  "description": "ds project | student manager",
  "main": "./dist/main.js",
  "build": {
    "appId": "com.dsproject.app",
    "productName": "dsproject",
    "target": "NSIS",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "files": [
      "*.js",
      "*.ts",
      "dist",
      "node_modules",
      "utils",
      "model"
    ]
  },
  "scripts": {
    "dev": "npm-run-all -p dev:react electron:serve",
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack/webpack.electron.config.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.react.config.js --mode development",
    "electron:serve": "npm run dev:electron",
    "start:electron": "electron .",
    "build": "npm-run-all build:electron build:react",
    "build:run": "npm-run-all build start:electron",
    "build:electron": "webpack --config webpack/webpack.electron.config.js",
    "build:react": "webpack --config webpack/webpack.react.config.js",
    "package": "npm-run-all build package:dist",
    "package:dist": "electron-builder --dir",
    "dist": "electron-builder",
    "test": "jest"
  },
  "author": "amirhossein sadeqi",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/electron": "^1.6.10",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.0",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "babel-loader": "^8.2.2",
    "class-transformer": "^0.3.2",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "electron": "^11.2.0",
    "electron-builder": "^22.9.1",
    "html-webpack-plugin": "^5.0.0-alpha.9",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "react-custom-scrollbars": "^4.2.1",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "wait-on": "^5.2.1",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
